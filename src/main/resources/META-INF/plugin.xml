<idea-plugin>
    <id>com.github.krepysh.branchswitcher</id>
    <name>Branch Switcher</name>
    <vendor>krepysh</vendor>
    <description>SSH config manager with ProxyJump support</description>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow factoryClass="com.github.krepysh.branchswitcher.toolWindow.MyToolWindowFactory" id="BranchSwitcher"/>
        <notificationGroup id="SSH Connection" displayType="BALLOON"/>
    </extensions>
</idea-plugin>
